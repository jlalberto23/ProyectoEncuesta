<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:layout_editor_absoluteX="1dp"
    tools:layout_editor_absoluteY="52dp">

    <Button
        android:id="@+id/consultarCuentabtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/consultar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.301"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrasena2"
        app:layout_constraintVertical_bias="0.739" />

    <EditText
        android:id="@+id/contrasena2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="48dp"
        android:ems="10"
        android:inputType="textPassword"
        app:layout_constraintTop_toBottomOf="@+id/userCreartxt"
        tools:layout_editor_absoluteX="170dp" />

    <EditText
        android:id="@+id/crearNomtxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="176dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.845"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/userCreartxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintTop_toBottomOf="@+id/crearNomtxt"
        tools:layout_editor_absoluteX="170dp" />

    <EditText
        android:id="@+id/carnettxt"
        android:layout_width="204dp"
        android:layout_height="37dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.769"
        app:layout_constraintStart_toEndOf="@+id/carnetlbl"
        tools:layout_editor_absoluteY="105dp" />

    <TextView
        android:id="@+id/carnetlbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:text="Carnet:"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="105dp" />

    <TextView
        android:id="@+id/nomlbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="192dp"
        android:text="Nombre completo:"
        app:layout_constraintEnd_toStartOf="@+id/crearNomtxt"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/userlbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="76dp"
        android:text="Usuario:"
        app:layout_constraintEnd_toStartOf="@+id/userCreartxt"
        app:layout_constraintHorizontal_bias="0.243"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nomlbl" />

    <TextView
        android:id="@+id/contralbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ContraseÃ±a:"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/contrasena2"
        app:layout_constraintHorizontal_bias="0.178"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/userlbl"
        app:layout_constraintVertical_bias="0.145" />

    <TextView
        android:id="@+id/tipolbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="196dp"
        android:text="Tipo de usuario"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tipoUsuarioList"
        app:layout_constraintHorizontal_bias="0.627"
        app:layout_constraintStart_toStartOf="parent" />

    <Spinner
        android:id="@+id/tipoUsuarioList"
        android:layout_width="211dp"
        android:layout_height="31dp"
        android:layout_marginTop="116dp"
        android:layout_marginEnd="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contrasena2" />

    <TextView
        android:id="@+id/fechaRegistrolbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Fecha de registro:"
        tools:layout_editor_absoluteX="20dp"
        tools:layout_editor_absoluteY="441dp" />

    <EditText
        android:id="@+id/fechaReg"
        android:layout_width="196dp"
        android:layout_height="38dp"
        android:ems="10"
        android:inputType="date"
        tools:layout_editor_absoluteX="169dp"
        tools:layout_editor_absoluteY="431dp" />

    <Button
        android:id="@+id/limpiarbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LIMPIAR"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.444"
        app:layout_constraintStart_toEndOf="@+id/consultarCuentabtn"
        tools:layout_editor_absoluteY="609dp" />

    <TextView
        android:id="@+id/consultarUserlbl"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CONSULTAR USUARIO"
        tools:layout_editor_absoluteX="143dp"
        tools:layout_editor_absoluteY="44dp" />

</androidx.constraintlayout.widget.ConstraintLayout>